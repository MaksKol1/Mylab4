@page
@model UploadModel
@{
    ViewData["Title"] = "Upload";
}

<h1>Upload Files</h1>

<form method="post" enctype="multipart/form-data">
    <div>
        <label for="file">Choose File:</label>
        <input type="file" id="file" name="files" multiple />
    </div>
    <br />
    <button type="submit">Upload</button>
</form>
